        <ownedAttribute xmi:id="_xajVdbpYEfC57dGcZbl7LA" name="nutritionistDAO" visibility="private" isLeaf="true" type="_wTx6o7pYEfC57dGcZbl7LA" isReadOnly="true" association="_xgRi9LpYEfC57dGcZbl7LA">
